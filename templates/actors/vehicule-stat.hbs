        <div class="part-vehicule  haut">
            {{formInput systemFields.haut name="system.haut" value=system.haut localize=true rootId=partId}}
        </div>
        <div class="part-vehicule droite">
            {{formInput systemFields.droite name="system.droite" value=system.droite localize=true rootId=partId}}
        </div>
        <div class="part-vehicule bas">
            {{formInput systemFields.bas name="system.bas" value=system.bas localize=true rootId=partId}}
        </div>
        <div class="part-vehicule gauche">
            {{formInput systemFields.gauche name="system.gauche" value=system.gauche localize=true rootId=partId}}
        </div>
        <div class="blindage infos">
            <div class="circle">
                <div class="fill">
                    {{formInput systemFields.armor name="system.armor" value=system.armor localize=true rootId=partId}}
                    {{formInput systemFields.armormax name="system.armormax" value=system.armormax localize=true rootId=partId}}
                </div>
            </div>
        </div>
        <div class="champ infos">
            <div class="circle">
                <div class="fill">
                    {{formInput systemFields.shield name="system.shield" value=system.shield localize=true rootId=partId}}
                    {{formInput systemFields.shieldmax name="system.shieldmax" value=system.shieldmax localize=true rootId=partId}}
                </div>
            </div>
        </div>
        <div class="moteur infos" data-percent="43">
            <div class="circle">
                <div class="fill">
                    {{formInput systemFields.health name="system.health" value=system.health localize=true rootId=partId}}
                    {{formInput systemFields.healthmax name="system.healthmax" value=system.healthmax localize=true rootId=partId}}
                </div>
            </div>
        </div>
        <div class="full infos">
            <div class="circle">
                <div class="fill">
                    {{formInput systemFields.enc name="system.enc" value=system.enc localize=true rootId=partId}}
                    {{formInput systemFields.encmax name="system.encmax" value=system.encmax localize=true rootId=partId}}
                </div>
            </div>
        </div>    
        <div class="credit-vehicule infos">
            {{formInput systemFields.credit name="system.credit" value=system.credit localize=true rootId=partId}}
        </div>
        <div class="info-arme">
        </div>
        <div class="deco"><div id="radar-container"><div id="radar"><div id="targets"></div></div></div></div>
    </div>
    <div class="inventory-list cut-corner">
        <div class="circles pilotage">
            {{{actor.system.sun1}}}
            <div class="technoinfo" title="{{localize 'Liber.Character.Competences.pilotage'}} / {{localize 'Liber.Character.IA.AI'}}"></div>
        </div>
        <div class="circles mecanique">
            {{{actor.system.sun2}}}
            <div class="technoinfo" title="{{localize 'Liber.Character.Competences.mecanique'}}"></div>
        </div>
        <div class="circles piratage">
            {{{actor.system.sun3}}}
            <div class="technoinfo" title="{{localize 'Liber.Character.Competences.piratage'}}"></div>
        </div>
        <div class="circles visee">
            {{{actor.system.sun4}}}
            <div class="technoinfo" title="{{localize 'Liber.Character.Competences.visee'}}"></div>
        </div>
    </div>
    <footer class="sheet-footer cut-corner">
        <div class="action">
          <h4>{{localize 'Liber.Character.Action.Pilote'}}</h4>
          <div class="evasive icone" title="{{localize 'Liber.Character.Action.Evasive'}}" data-action="fulldown" data-full="10" data-name="evasive"></div>
          <div class="pique icone" title="{{localize 'Liber.Character.Action.Pique'}}" data-action="fulldown" data-full="20" data-name="pique"></div>
          <div class="leurre icone" title="{{localize 'Liber.Character.Action.Leurre'}}" data-action="fulldown" data-full="10" data-name="leurre"></div>
          <div class="invisible icone" title="{{localize 'Liber.Character.Action.Invisible'}}" data-action="fulldown" data-full="30" data-name="invisible"></div>
          <div class="tactique icone" title="{{localize 'Liber.Character.Action.Tactique'}}" data-action="fulldown" data-full="10" data-name="tactique"></div>
          <div class="sprint icone" title="{{localize 'Liber.Character.Action.Sprint'}}" data-action="fulldown" data-full="50" data-name="sprint"></div>
        </div>
        <div class="action">
          <h4>{{localize 'Liber.Character.Action.Passager'}}</h4>
          <div class="shoot icone" title="{{localize 'Liber.Character.Action.Secondaire'}}" data-action="fulldown" data-full="5" data-name="shoot"></div>
          <div class="repair icone" title="{{localize 'Liber.Character.Action.Repair'}}" data-action="fulldown" data-full="20" data-name="repair"></div>
          <div class="surcharge icone" title="{{localize 'Liber.Character.Action.Surcharge'}}" data-action="fulldown" data-full="20" data-name="surcharge"></div>
          <div class="radar icone" title="{{localize 'Liber.Character.Action.Radar'}}" data-action="fulldown" data-full="5" data-name="radar"></div>
          <div class="radio icone" title="{{localize 'Liber.Character.Action.Radio'}}" data-action="fulldown" data-full="5" data-name="radio"></div>
          <div class="medic icone" title="{{localize 'Liber.Character.Action.Medic'}}" data-action="fulldown" data-full="10" data-name="medic"></div>
          <div class="urgence icone" title="{{localize 'Liber.Character.Action.Urgence'}}" data-action="fulldown" data-full="5" data-name="urgence"></div>
          <div class="sabotage icone" title="{{localize 'Liber.Character.Action.Sabotage'}}" data-action="fulldown" data-full="20" data-name="sabotage"></div>
          <div class="abordage icone" title="{{localize 'Liber.Character.Action.Abordage'}}" data-action="fulldown" data-full="40" data-name="abordage"></div>
        </div>
    </footer>
</div>